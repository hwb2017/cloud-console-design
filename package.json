{
  "name": "cloud-console-design",
  "version": "0.1.8",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hwb2017/cloud-console-design.git"
  },
  "main": "dist/index.bundle.js",
  "module": "dist/components/index.js",
  "unpkg": "dist/index.min.js",
  "style": "dist/style/index.css",
  "files": [
    "dist",
    "components"
  ],
  "sideEffects": [
    "dist/style/*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:bundle && npm run build:components",
    "build:bundle": "rollup --config ./build/rollup.config.bundle.js",
    "build:components": "node build/individualComponents.js",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "prepublishOnly": "node scripts/prepublish.js"
  },
  "dependencies": {
    "@popperjs/core": "^2.10.2",
    "async-validator": "^4.0.7",
    "lodash-es": "^4.17.21",
    "vue-types": "^4.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.2.4",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "autoprefixer": "^10.3.3",
    "chalk": "^4.1.2",
    "cssnano": "^5.0.8",
    "esbuild": "^0.12.26",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "lint-staged": "^11.1.1",
    "postcss": "^8.3.6",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-esbuild": "^4.5.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.38.1",
    "sass-loader": "^8.0.2",
    "typescript": "~4.1.5",
    "vue-jest": "^5.0.0-0"
  },
  "peerDependencies": {
    "vue": "^3.2.6"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "engines": {
    "node": ">= 14"
  }
}
